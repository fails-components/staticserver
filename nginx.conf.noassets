server { 
 listen 80;
 server_name frontend;
 port_in_redirect off;

 location /static/app {
   root /usr/share/nginx/html;
   index index.html
   try_files $uri /index.html;
 }
 location /static/lecture {
   root /usr/share/nginx/html;
   index index.html
   try_files $uri /index.html;
 }

  location /static/oss {
   root /usr/share/nginx/html;
   index attribution.txt
   try_files $uri /attribution.txt;
 }
 

}